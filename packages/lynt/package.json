{
  "name": "lynt",
  "description": "A zero config JavaScript linter",
  "author": "Saad Quadri",
  "license": "MIT",
  "version": "0.0.1-alpha",
  "main": "lib/index.js",
  "bin": {
    "lynt": "lib/cli.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "trash lib",
    "dev": "tsc --watch",
    "format": "prettier \"src/**/*.ts\" --write --no-semi --single-quote",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@types/eslint": "^4.16.0",
    "@types/globby": "^6.1.0",
    "@types/meow": "^4.0.1",
    "@types/node": "^9.6.5",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-react": "^7.7.0",
    "globby": "^8.0.1",
    "meow": "^4.0.0",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.5.1"
  },
  "devDependencies": {
    "prettier": "^1.12.1",
    "trash-cli": "^1.4.0",
    "typescript": "^2.8.3"
  },
  "peerDependencies": {
    "typescript": ">=2.1.0 || >=2.1.0-dev || >=2.2.0-dev || >=2.3.0-dev || >=2.4.0-dev || >=2.5.0-dev || >=2.6.0-dev || >=2.7.0-dev || >=2.8.0-dev || >=2.9.0-dev"
  },
  "keywords": [
    "eslint",
    "lint",
    "linter",
    "lynt",
    "tslint"
  ],
  "typings": "lib/index.d.ts",
  "optionalDependencies": {}
}
